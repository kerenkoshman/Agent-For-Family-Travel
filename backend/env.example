# Server Configuration
NODE_ENV=development
PORT=3001
FRONTEND_URL=http://localhost:5173

# Database Configuration
DATABASE_URL=postgresql://username:password@localhost:5432/family_trip_agent

# Session Configuration
SESSION_SECRET=your-super-secret-session-key-change-this-in-production

# JWT Configuration
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production
JWT_EXPIRES_IN=7d

# Google OAuth Configuration
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here

# External API Keys (Optional - for development)
TRIPADVISOR_API_KEY=your_tripadvisor_api_key_here
GOOGLE_PLACES_API_KEY=your_google_places_api_key_here
SKYSCANNER_API_KEY=your_skyscanner_api_key_here
BOOKING_API_KEY=your_booking_api_key_here

# Feature Flags
ENABLE_LOGGING=true
ENABLE_CORS=true
ENABLE_RATE_LIMITING=true
